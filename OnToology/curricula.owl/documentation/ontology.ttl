@prefix : <https://w3id.org/ontocis/curricula#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@base <https://w3id.org/ontocis/curricula> .

<https://w3id.org/ontocis/curricula> rdf:type owl:Ontology ;
                                      owl:imports <https://w3id.org/ontocis/competences> ,
                                                  <https://w3id.org/ontocis/composition> ,
                                                  <https://w3id.org/ontocis/courses> ,
                                                  <https://w3id.org/ontocis/degrees> ,
                                                  <https://w3id.org/ontocis/professors> ,
                                                  <https://w3id.org/ontocis/topics> ;
                                      <http://purl.org/dc/terms/#license> <https://opensource.org/licenses/MIT> ;
                                      rdfs:comment "An ontology that describes the curricula of the degree in Software Engineering at Universidad ORT Uruguay."@en ,
                                                   "Una ontología que describe el currículo de la carrera Ingeniería en Sistemas en la Universidad ORT Uruguay."@es ;
                                      rdfs:label "OntoCIS"@en ,
                                                 "OntoCIS"@es .

#################################################################
#    Annotation properties
#################################################################

###  https://w3id.org/ontocis/topics#altLabel
<https://w3id.org/ontocis/topics#altLabel> rdf:type owl:AnnotationProperty ;
                                           rdfs:comment "Agrega sinónimos, términos relacionados o variantes del término includo en la anotación rdfs:label."@es ,
                                                        "Used for synoyms, related terms or different combinantions of the term that appears in the rdfs:label annotation."@en ;
                                           rdfs:label "alternative label"@en ,
                                                      "etiqueta alternativa"@es .


#################################################################
#    Object Properties
#################################################################

###  http://xmlns.com/foaf/0.1/member
<http://xmlns.com/foaf/0.1/member> rdf:type owl:ObjectProperty .


###  https://w3id.org/ontocis/competences#contributesTo
<https://w3id.org/ontocis/competences#contributesTo> rdf:type owl:ObjectProperty ;
                                                     rdfs:domain <https://w3id.org/ontocis/courses#Course> ;
                                                     rdfs:range <https://w3id.org/ontocis/competences#Competence> ;
                                                     rdfs:comment "Indica el grado de contribución al desarrollo de una competencia"@es ,
                                                                  "It states the degree of contribution to the development of a competency"@en ;
                                                     rdfs:label "contributes to"@en ,
                                                                "contribuye a"@es .


###  https://w3id.org/ontocis/competences#develops
<https://w3id.org/ontocis/competences#develops> rdf:type owl:ObjectProperty ;
                                                rdfs:subPropertyOf <https://w3id.org/ontocis/competences#contributesTo> ;
                                                rdfs:comment "Indica que la competencia es total o parcialmente desarrollada a lo largo de una asignatura, es evaluada en la asignatura y se cuenta con evidencia de la evaluación."@es ,
                                                             "It states that the competency is totally or partially developed in a course, it is assessed and there is evidence of the assessment."@en ;
                                                rdfs:label "desarrolla"@es ,
                                                           "develops"@en .


###  https://w3id.org/ontocis/competences#hasLearningOutcome
<https://w3id.org/ontocis/competences#hasLearningOutcome> rdf:type owl:ObjectProperty ;
                                                          rdfs:subPropertyOf owl:topObjectProperty ;
                                                          rdfs:domain <https://w3id.org/ontocis/courses#Course> ;
                                                          rdfs:range <https://w3id.org/ontocis/competences#Learning_Outcome> ;
                                                          rdfs:comment "Indica que objetivos de aprendizaje tiene una cosa."@es ,
                                                                       "It states the learning outcomes for a certain thing."@en ;
                                                          rdfs:label "has learning outcome"@en ,
                                                                     "tiene como objetivo de aprendizaje"@es .


###  https://w3id.org/ontocis/competences#partiallyDevelops
<https://w3id.org/ontocis/competences#partiallyDevelops> rdf:type owl:ObjectProperty ;
                                                         rdfs:subPropertyOf <https://w3id.org/ontocis/competences#contributesTo> ;
                                                         rdfs:comment "Indica que la asignatura contribuye de alguna manera al desarrollo de la competencia, pero no esta no es evaluada."@es ,
                                                                      "It states that the course contributes to the development of the competency but it is not assessed."@en ;
                                                         rdfs:label "desarrolla parcialmente"@es ,
                                                                    "partially develops"@en .


###  https://w3id.org/ontocis/composition#partOf
<https://w3id.org/ontocis/composition#partOf> rdf:type owl:ObjectProperty ;
                                              rdfs:subPropertyOf owl:topObjectProperty ;
                                              rdf:type owl:TransitiveProperty ;
                                              rdfs:comment "Indica la composición entre individuos"@es ,
                                                           "It states part-whole relations"@en ;
                                              rdfs:label "part of"@en ,
                                                         "parte de"@es .


###  https://w3id.org/ontocis/courses#isPrerequisite
<https://w3id.org/ontocis/courses#isPrerequisite> rdf:type owl:ObjectProperty ;
                                                  rdfs:subPropertyOf owl:topObjectProperty ;
                                                  rdf:type owl:TransitiveProperty ;
                                                  rdfs:domain <https://w3id.org/ontocis/courses#Course> ;
                                                  rdfs:range <https://w3id.org/ontocis/courses#Course> ;
                                                  rdfs:comment "Indica que asignaturas debe haber cursado un estudiante para poder inscribirse o aprovar una asignatura."@es ,
                                                               "It states which courses the student needs to take in order to be allowed to enroll or pass a course."@en ;
                                                  rdfs:label "es previa"@es ,
                                                             "is prerequiste"@en .


###  https://w3id.org/ontocis/courses#isPrerequisiteToEnroll
<https://w3id.org/ontocis/courses#isPrerequisiteToEnroll> rdf:type owl:ObjectProperty ;
                                                          rdfs:subPropertyOf <https://w3id.org/ontocis/courses#isPrerequisite> ;
                                                          rdfs:comment """Indica que asignaturas que deben haber cursado el estudiante para poder inscribirse a una asignatura.
Solamente si el estudiante ha obtenido el crédito parcial o total de estas asignaturas, según corresponda, podrá inscribirse a la asignatura y eventualmente obtener el crédito parcial para el mismo."""@es ,
                                                                       """It states which courses a student needs to take before hand in order to be allowed to enroll for a course.
Only if the student has obtained partial or total credit for these subjects will he be able to enroll in the course and eventually obtain partial credit for it."""@en ;
                                                          rdfs:label "es previa para inscribirse"@es ,
                                                                     "is prerequisite to enroll"@en .


###  https://w3id.org/ontocis/courses#isPrerequisiteToPass
<https://w3id.org/ontocis/courses#isPrerequisiteToPass> rdf:type owl:ObjectProperty ;
                                                        rdfs:subPropertyOf <https://w3id.org/ontocis/courses#isPrerequisite> ;
                                                        rdfs:comment "Indica que asignaturas debe haber cursado previamente un estudiante para poder aprobar (obtener el crédito total) una asignatura."@es ,
                                                                     "It states which courses the student needs to take before hand in order to be able to pass (get the total credits) a certain course."@en ;
                                                        rdfs:label "es previa para aprobar"@es ,
                                                                   "is prerequisite to pass"@en .


###  https://w3id.org/ontocis/courses#partialCreditIsPrerequisiteToEnroll
<https://w3id.org/ontocis/courses#partialCreditIsPrerequisiteToEnroll> rdf:type owl:ObjectProperty ;
                                                                       rdfs:subPropertyOf <https://w3id.org/ontocis/courses#isPrerequisiteToEnroll> ;
                                                                       rdfs:comment "Indica para que asignaturas el estudiante debe tener el crédito parcial antes de poder inscribirse a una asignatura."@es ,
                                                                                    "It states which courses a student must have the partial credit for in order to enroll to another course."@en ;
                                                                       rdfs:label "credito parcial es previa para inscribirse"@es ,
                                                                                  "partial credit is prerequisite to enroll"@en .


###  https://w3id.org/ontocis/courses#partialCreditIsPrerequisiteToPass
<https://w3id.org/ontocis/courses#partialCreditIsPrerequisiteToPass> rdf:type owl:ObjectProperty ;
                                                                     rdfs:subPropertyOf <https://w3id.org/ontocis/courses#isPrerequisiteToPass> ;
                                                                     rdfs:comment "Indica para que asignaturas el estudiante debe tener el crédito parcial antes de poder inscribirse a una asignatura."@es ,
                                                                                  "It states which courses a student must have the partial credit for in order to pass (obtain total credits) another course."@en ;
                                                                     rdfs:label "credito parcial es previa para aprobar"@es ,
                                                                                "partial credit is prerequisite to pass"@en .


###  https://w3id.org/ontocis/courses#totalCreditIsPrerequisiteToEnroll
<https://w3id.org/ontocis/courses#totalCreditIsPrerequisiteToEnroll> rdf:type owl:ObjectProperty ;
                                                                     rdfs:subPropertyOf <https://w3id.org/ontocis/courses#isPrerequisiteToEnroll> ;
                                                                     rdfs:comment "Indica para que asignaturas el estudiante debe tener el crédito total antes de poder inscribirse a una asignatura."@es ,
                                                                                  "It states which courses a student must have the total credit for in order to enroll to another course."@en ;
                                                                     rdfs:label "credito total es previa para inscribirse"@es ,
                                                                                "total credit is prerequisite to enroll"@en .


###  https://w3id.org/ontocis/courses#totalCreditIsPrerequisiteToPass
<https://w3id.org/ontocis/courses#totalCreditIsPrerequisiteToPass> rdf:type owl:ObjectProperty ;
                                                                   rdfs:subPropertyOf <https://w3id.org/ontocis/courses#isPrerequisiteToPass> ;
                                                                   rdfs:comment "Indica para que asignaturas el estudiante debe tener el crédito total antes de poder inscribirse a una asignatura."@es ,
                                                                                "It states which courses a student must have the total credit for in order to pass (obtain total credits) another course."@en ;
                                                                   rdfs:label "credito total es previa para aprobar"@es ,
                                                                              "total credit is prerequisite to pass"@en .


###  https://w3id.org/ontocis/degrees#requiredFor
<https://w3id.org/ontocis/degrees#requiredFor> rdf:type owl:ObjectProperty ;
                                               rdfs:domain <https://w3id.org/ontocis/courses#Course> ;
                                               rdfs:range <https://w3id.org/ontocis/degrees#Degree> ;
                                               rdfs:comment "Indica que se requiere para que un estudiante obtenga un título."@es ,
                                                            "It states what is required for the student in order to obtain a certain degree."@en ;
                                               rdfs:label "requerido para"@es ,
                                                          "required for"@en .


###  https://w3id.org/ontocis/professors#hasEducationLevel
<https://w3id.org/ontocis/professors#hasEducationLevel> rdf:type owl:ObjectProperty ;
                                                        rdfs:subPropertyOf <http://xmlns.com/foaf/0.1/member> ;
                                                        rdfs:domain <https://w3id.org/ontocis/professors#Person> ;
                                                        rdfs:range <https://w3id.org/ontocis/professors#Education_Level> ;
                                                        rdfs:comment "Indica el nivel de formación más alto alcanzado por una persona."@es ,
                                                                     "It states the highest degree aquired by a person."@en ;
                                                        rdfs:label "has education level"@en ,
                                                                   "tiene nivel de formación"@es .


###  https://w3id.org/ontocis/professors#hasNationality
<https://w3id.org/ontocis/professors#hasNationality> rdf:type owl:ObjectProperty ;
                                                     rdfs:subPropertyOf <http://xmlns.com/foaf/0.1/member> ;
                                                     rdfs:domain <https://w3id.org/ontocis/professors#Person> ;
                                                     rdfs:range <https://w3id.org/ontocis/professors#Nationality> ;
                                                     rdfs:comment "Indica la nacionalidad de una persona."@es ,
                                                                  "It states the nationality of a person."@en ;
                                                     rdfs:label "has nationality"@en ,
                                                                "tiene nacionalidad"@es .


###  https://w3id.org/ontocis/professors#inChargeOf
<https://w3id.org/ontocis/professors#inChargeOf> rdf:type owl:ObjectProperty ;
                                                 rdfs:subPropertyOf owl:topObjectProperty ;
                                                 rdfs:domain <https://w3id.org/ontocis/professors#Person> ;
                                                 rdfs:range <https://w3id.org/ontocis/courses#Course> ;
                                                 rdfs:comment "Indica que una persona esta a cargo de algo."@es ,
                                                              "It states that a person is in charge of something."@en ;
                                                 rdfs:label "a cargo de"@es ,
                                                            "in charge of"@en .


###  https://w3id.org/ontocis/topics#relatedTo
<https://w3id.org/ontocis/topics#relatedTo> rdf:type owl:ObjectProperty ;
                                            rdfs:subPropertyOf owl:topObjectProperty ;
                                            rdf:type owl:SymmetricProperty ;
                                            rdfs:domain <https://w3id.org/ontocis/topics#Topic> ;
                                            rdfs:range <https://w3id.org/ontocis/topics#Topic> ;
                                            rdfs:comment "Indica que un tema esta relacionado con otro tema"@es ,
                                                         "It states that a topic is related to another topic"@en ;
                                            rdfs:label "relacionado con"@es ,
                                                       "related to"@en .


###  https://w3id.org/ontocis/topics#subtopicOf
<https://w3id.org/ontocis/topics#subtopicOf> rdf:type owl:ObjectProperty ;
                                             rdfs:subPropertyOf <https://w3id.org/ontocis/topics#relatedTo> ;
                                             rdf:type owl:TransitiveProperty ;
                                             rdfs:domain <https://w3id.org/ontocis/topics#Topic> ;
                                             rdfs:range <https://w3id.org/ontocis/topics#Topic> ;
                                             rdfs:comment "Indica que un tema es parte de otro tema más general"@es ,
                                                          "It states that a topic is part of a broader or more general topic"@en ;
                                             rdfs:label "subtema de"@es ,
                                                        "subtopic of"@en .


###  https://w3id.org/ontocis/topics#taughtIn
<https://w3id.org/ontocis/topics#taughtIn> rdf:type owl:ObjectProperty ;
                                           rdfs:subPropertyOf owl:topObjectProperty ;
                                           rdfs:domain <https://w3id.org/ontocis/topics#Topic> ;
                                           rdfs:range <https://w3id.org/ontocis/courses#Course> ;
                                           rdfs:comment "Indica que un tema se eseña o dicta durante una asignatura"@es ,
                                                        "It states that a topic is taught during a course"@en ;
                                           rdfs:label "enseñado en"@es ,
                                                      "taught in"@en .


#################################################################
#    Data properties
#################################################################

###  http://xmlns.com/foaf/0.1/familyName
<http://xmlns.com/foaf/0.1/familyName> rdf:type owl:DatatypeProperty ;
                                       owl:equivalentProperty <https://w3id.org/ontocis/professors#surname> .


###  http://xmlns.com/foaf/0.1/givenName
<http://xmlns.com/foaf/0.1/givenName> rdf:type owl:DatatypeProperty ;
                                      owl:equivalentProperty <https://w3id.org/ontocis/professors#name> .


###  https://w3id.org/ontocis/courses#credits
<https://w3id.org/ontocis/courses#credits> rdf:type owl:DatatypeProperty ;
                                           rdfs:subPropertyOf owl:topDataProperty ;
                                           rdf:type owl:FunctionalProperty ;
                                           rdfs:domain <https://w3id.org/ontocis/courses#Course> ;
                                           rdfs:range [ rdf:type rdfs:Datatype ;
                                                        owl:onDatatype xsd:integer ;
                                                        owl:withRestrictions ( [ xsd:minExclusive 0
                                                                               ]
                                                                             )
                                                      ] ;
                                           rdfs:comment "Amount of credits obtained when approving a course."@en ,
                                                        "Cantidad de créditos que se obtienen al aprobar una asignatura."@es ;
                                           rdfs:label "credits"@en ,
                                                      "créditos"@es .


###  https://w3id.org/ontocis/courses#hours
<https://w3id.org/ontocis/courses#hours> rdf:type owl:DatatypeProperty ;
                                         rdfs:subPropertyOf owl:topDataProperty ;
                                         rdf:type owl:FunctionalProperty ;
                                         rdfs:domain <https://w3id.org/ontocis/courses#Course> ;
                                         rdfs:range [ rdf:type rdfs:Datatype ;
                                                      owl:onDatatype xsd:integer ;
                                                      owl:withRestrictions ( [ xsd:minExclusive 0
                                                                             ]
                                                                           )
                                                    ] ;
                                         rdfs:comment "Amount of hours taught for a course."@es ,
                                                      "Cantidad de horas que se dictan para una asignatura"@es ;
                                         rdfs:label "horas"@es ,
                                                    "hours"@en .


###  https://w3id.org/ontocis/courses#minimumCreditsRequired
<https://w3id.org/ontocis/courses#minimumCreditsRequired> rdf:type owl:DatatypeProperty ;
                                                          rdfs:subPropertyOf owl:topDataProperty ;
                                                          rdfs:domain <https://w3id.org/ontocis/courses#Course> ;
                                                          rdfs:range [ rdf:type rdfs:Datatype ;
                                                                       owl:onDatatype xsd:integer ;
                                                                       owl:withRestrictions ( [ xsd:minExclusive 0
                                                                                              ]
                                                                                            )
                                                                     ] ;
                                                          rdfs:comment "Cantidad mínima de créditos requeridos para poder cursar una asignatura."@es ,
                                                                       "Minumum number of credits required in order to take a course"@en ;
                                                          rdfs:label "minimo de creditos requeridos"@es ,
                                                                     "minimum credits required"@en .


###  https://w3id.org/ontocis/courses#partialCreditScore
<https://w3id.org/ontocis/courses#partialCreditScore> rdf:type owl:DatatypeProperty ;
                                                      rdfs:subPropertyOf owl:topDataProperty ;
                                                      rdf:type owl:FunctionalProperty ;
                                                      rdfs:domain <https://w3id.org/ontocis/courses#Course> ;
                                                      rdfs:range [ rdf:type rdfs:Datatype ;
                                                                   owl:onDatatype xsd:integer ;
                                                                   owl:withRestrictions ( [ xsd:minInclusive 0
                                                                                          ]
                                                                                          [ xsd:maxInclusive 100
                                                                                          ]
                                                                                        )
                                                                 ] ;
                                                      rdfs:comment "Minimum score required to obtain the partial credit for a course and have be allowed to sit an exam."@en ,
                                                                   "Puntaje mínimo que debe alcanzarse para obtener el crédito parcial de una asignatura, y tener derecho a dar examen."@es ;
                                                      rdfs:label "partial credit score"@en ,
                                                                 "puntaje para crédito parcial"@es .


###  https://w3id.org/ontocis/courses#totalCreditScore
<https://w3id.org/ontocis/courses#totalCreditScore> rdf:type owl:DatatypeProperty ;
                                                    rdfs:subPropertyOf owl:topDataProperty ;
                                                    rdf:type owl:FunctionalProperty ;
                                                    rdfs:domain <https://w3id.org/ontocis/courses#Course> ;
                                                    rdfs:range [ rdf:type rdfs:Datatype ;
                                                                 owl:onDatatype xsd:integer ;
                                                                 owl:withRestrictions ( [ xsd:minInclusive 0
                                                                                        ]
                                                                                        [ xsd:maxInclusive 100
                                                                                        ]
                                                                                      )
                                                               ] ;
                                                    rdfs:comment "Minimum score requiered to approve a course."@es ,
                                                                 "Puntaje mínimo requerido para aprobar una asignatura."@es ;
                                                    rdfs:label "puntaje para crédito total"@es ,
                                                               "total credit score"@en .


###  https://w3id.org/ontocis/professors#isExperienced
<https://w3id.org/ontocis/professors#isExperienced> rdf:type owl:DatatypeProperty ;
                                                    rdfs:subPropertyOf owl:topDataProperty ;
                                                    rdf:type owl:FunctionalProperty ;
                                                    rdfs:domain <https://w3id.org/ontocis/professors#Professor> ;
                                                    rdfs:range xsd:boolean ;
                                                    rdfs:comment "Has more than 5 years of experience teaching or researching."@en ,
                                                                 "Tiene más de 5 años de experience dictando clases o investigando."@es ;
                                                    rdfs:label "is experienced"@en ,
                                                               "tiene experiencia"@es .


###  https://w3id.org/ontocis/professors#name
<https://w3id.org/ontocis/professors#name> rdf:type owl:DatatypeProperty ;
                                           rdfs:subPropertyOf owl:topDataProperty ;
                                           rdf:type owl:FunctionalProperty ;
                                           rdfs:domain <https://w3id.org/ontocis/professors#Person> ;
