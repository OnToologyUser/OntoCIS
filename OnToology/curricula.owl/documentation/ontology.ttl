###  https://w3id.org/ontocis/professors#surname
<https://w3id.org/ontocis/professors#surname> rdf:type owl:DatatypeProperty ;
                                              rdfs:subPropertyOf owl:topDataProperty ;
                                              rdf:type owl:FunctionalProperty ;
                                              rdfs:domain <https://w3id.org/ontocis/professors#Person> ;
                                              rdfs:range xsd:string ;
                                              rdfs:comment "El apellido de una persona"@es ,
                                                           "The family name of some person"@en ;
                                              rdfs:label "apellido"@es ,
                                                         "surname"@en .


#################################################################
#    Classes
#################################################################

###  http://www.w3.org/2004/02/skos/core#Concept
<http://www.w3.org/2004/02/skos/core#Concept> rdf:type owl:Class ;
                                              owl:equivalentClass <https://w3id.org/ontocis/topics#Topic> .


###  http://xmlns.com/foaf/0.1/Group
<http://xmlns.com/foaf/0.1/Group> rdf:type owl:Class .


###  http://xmlns.com/foaf/0.1/Person
<http://xmlns.com/foaf/0.1/Person> rdf:type owl:Class ;
                                   owl:equivalentClass <https://w3id.org/ontocis/professors#Person> .


###  https://w3id.org/ontocis/competences#Category
<https://w3id.org/ontocis/competences#Category> rdf:type owl:Class ;
                                                rdfs:comment "Categories under which the competences are organized"@en ,
                                                             "Categorías bajo las cuales se organizan las competencias"@es ;
                                                rdfs:label "Category"@en ,
                                                           "Categoría"@es .


###  https://w3id.org/ontocis/competences#Competence
<https://w3id.org/ontocis/competences#Competence> rdf:type owl:Class ;
                                                  owl:equivalentClass [ rdf:type owl:Restriction ;
                                                                        owl:onProperty <https://w3id.org/ontocis/composition#partOf> ;
                                                                        owl:someValuesFrom <https://w3id.org/ontocis/competences#Category>
                                                                      ] ;
                                                  rdfs:comment "A general statement detailing the desired knowledge and skills of student graduating from our course or program."@en ,
                                                               "Una frase que describe de manera general el conocimiento y capacidades que debe adquirir un estudiante."@es ;
                                                  rdfs:label "Competencia"@es ,
                                                             "Competency"@en .


###  https://w3id.org/ontocis/competences#Learning_Outcome
<https://w3id.org/ontocis/competences#Learning_Outcome> rdf:type owl:Class ;
                                                        owl:equivalentClass [ rdf:type owl:Restriction ;
                                                                              owl:onProperty <https://w3id.org/ontocis/composition#partOf> ;
                                                                              owl:someValuesFrom <https://w3id.org/ontocis/competences#Competence>
                                                                            ] ;
                                                        rdfs:comment "A very specific statement that describes exactly what a student will be able to do in some measurable way"@en ,
                                                                     "Una frase que describe exactamente lo que un estudiante podrá hacer de manera mensurable/evaluable."@es ;
                                                        rdfs:label "Learning Outcome"@en ,
                                                                   "Objetivo de Aprendizaje"@es .


###  https://w3id.org/ontocis/courses#Core_Course
<https://w3id.org/ontocis/courses#Core_Course> rdf:type owl:Class ;
                                               rdfs:subClassOf <https://w3id.org/ontocis/courses#Course> ;
                                               owl:disjointWith <https://w3id.org/ontocis/courses#Elective_Course> ;
                                               rdfs:comment "Asignatura que los estudiantes deben cursar como parte de la currícula para obtener un título."@es ,
                                                            "Course that the students must attend as part of the curricula to obtain a degree."@en ;
                                               rdfs:label "Asignatura Obligatoria"@es ,
                                                          "Core Course"@en .


###  https://w3id.org/ontocis/courses#Course
<https://w3id.org/ontocis/courses#Course> rdf:type owl:Class ;
                                          owl:equivalentClass [ rdf:type owl:Class ;
                                                                owl:unionOf ( <https://w3id.org/ontocis/courses#Core_Course>
                                                                              <https://w3id.org/ontocis/courses#Elective_Course>
